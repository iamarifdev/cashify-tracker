{
  "name": "cashify",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run gen:routes && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "test": "echo \"No tests specified\" && exit 0",
    "gen:routes": "npx @tanstack/router-cli generate",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 4173 --host",
    "deploy": "firebase deploy",
    "prepare": "husky install",
    "precommit": "npm run typecheck && npm run lint && npm run build && npm test",
    "check-all": "npm run typecheck && npm run lint && npm run build"
  },
  "dependencies": {
    "@tanstack/react-form": "^1.27.1",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/router-cli": "^1.139.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase-tools": "^14.26.0",
    "husky": "^8.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
